    "node": ">= 6"
  },
  "ava": {
    "babel": false
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/random-n/issues"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "codecov": "^3.2.0",
    "eslint": "^5.14.0",
    "eslint-config-ostai": "^1.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nyc": "^13.3.0"
  }
}
